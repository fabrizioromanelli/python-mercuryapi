echo Testing tag memory operations

set /reader/gen2/session S0
set /reader/gen2/accessPassword 0

echo Write EPC
writeepc EPC:0123456789ABCDEF01234567
set /reader/read/plan [GEN2,[],EPC:0123456789ABCDEF01234567,1]
read 100
set /reader/read/plan [GEN2,[],NULL,1]

echo Read tag memory from Reserved (password) bank
echo Unsingulated
writemembytes 0 0 bytes:0000000000000000 EPC:0123456789ABCDEF01234567
readmembytes 0 0 8
writemembytes 0 0 bytes:0011223344556677 EPC:0123456789ABCDEF01234567
readmembytes 0 0 8

echo Singulated.  Try all legal lengths
readmembytes 0 0 1 EPC:0123456789ABCDEF01234567
readmembytes 0 0 2 EPC:0123456789ABCDEF01234567
readmembytes 0 0 3 EPC:0123456789ABCDEF01234567
readmembytes 0 0 4 EPC:0123456789ABCDEF01234567
readmembytes 0 0 5 EPC:0123456789ABCDEF01234567
readmembytes 0 0 6 EPC:0123456789ABCDEF01234567
readmembytes 0 0 7 EPC:0123456789ABCDEF01234567
readmembytes 0 0 8 EPC:0123456789ABCDEF01234567

echo Singulated. Try non-word boundaries
readmembytes 0 1 1 EPC:0123456789ABCDEF01234567
readmembytes 0 1 2 EPC:0123456789ABCDEF01234567
readmembytes 0 1 3 EPC:0123456789ABCDEF01234567
readmembytes 0 1 4 EPC:0123456789ABCDEF01234567
readmembytes 0 1 5 EPC:0123456789ABCDEF01234567
readmembytes 0 1 6 EPC:0123456789ABCDEF01234567
readmembytes 0 1 7 EPC:0123456789ABCDEF01234567

echo Unsingulated
readmemwords 0 0 4
echo Singulated.  Try all legal lengths
readmemwords 0 0 1 EPC:0123456789ABCDEF01234567
readmemwords 0 0 2 EPC:0123456789ABCDEF01234567
readmemwords 0 0 3 EPC:0123456789ABCDEF01234567
readmemwords 0 0 4 EPC:0123456789ABCDEF01234567
readmemwords 0 1 1 EPC:0123456789ABCDEF01234567
readmemwords 0 1 2 EPC:0123456789ABCDEF01234567
readmemwords 0 1 3 EPC:0123456789ABCDEF01234567

echo Read EPC bank
readmembytes 1 0 16 EPC:0123456789ABCDEF01234567
readmemwords 1 0 8 EPC:0123456789ABCDEF01234567

echo Write Reserved bank
writemembytes 0 0 bytes:12345678 EPC:0123456789ABCDEF01234567
readmembytes 0 0 8 EPC:0123456789ABCDEF01234567
writemembytes 0 0 bytes:00000000 EPC:0123456789ABCDEF01234567
readmembytes 0 0 8 EPC:0123456789ABCDEF01234567

echo Partial write of reserved bank
writemembytes 0 0 bytes:123456 EPC:0123456789ABCDEF01234567
readmembytes 0 0 8 EPC:0123456789ABCDEF01234567
writemembytes 0 2 bytes:BEEF EPC:0123456789ABCDEF01234567
readmembytes 0 0 8 EPC:0123456789ABCDEF01234567
writemembytes 0 6 bytes:CAFE EPC:0123456789ABCDEF01234567
readmembytes 0 0 8 EPC:0123456789ABCDEF01234567
writemembytes 0 0 bytes:DEAD EPC:0123456789ABCDEF01234567
readmembytes 0 0 8 EPC:0123456789ABCDEF01234567
writemembytes 0 4 bytes:BABE EPC:0123456789ABCDEF01234567
readmembytes 0 0 8 EPC:0123456789ABCDEF01234567

echo Write by words
writememwords 0 0 words:12345678 EPC:0123456789ABCDEF01234567
readmemwords 0 0 4 EPC:0123456789ABCDEF01234567
writememwords 0 0 words:0000 EPC:0123456789ABCDEF01234567
readmemwords 0 0 4 EPC:0123456789ABCDEF01234567
writememwords 0 1 words:0000 EPC:0123456789ABCDEF01234567
readmemwords 0 0 4 EPC:0123456789ABCDEF01234567
writememwords 0 2 words:0000 EPC:0123456789ABCDEF01234567
readmemwords 0 0 4 EPC:0123456789ABCDEF01234567
writememwords 0 3 words:0000 EPC:0123456789ABCDEF01234567
readmemwords 0 0 4 EPC:0123456789ABCDEF01234567
